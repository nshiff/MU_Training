<?xml version="1.0" encoding="UTF-8"?>
<config>

	<modules>
		<Training_Practice>
			<version>0.1.0</version>
		</Training_Practice>
	</modules>

	<frontend>
		<routers>
			<training_practice>
				<use>standard</use>
				<args>
					<module>Training_Practice</module>
					<frontName>render</frontName>
				</args>
			</training_practice>
		</routers>
	</frontend>
	<global>
		<models>
			<training_practice>
				<class>Training_Practice_Model</class>
			</training_practice>
		</models>

		<blocks>
			<training_practice>
				<class>Training_Practice_Block</class>	<!-- app/code/local/Training/Practice/Block -->
			</training_practice>
			<page>
				<rewrite>
					<html_breadcrumbs>Training_Practice_Block_Page_Breadcrumbs</html_breadcrumbs>
				</rewrite>
			</page>
		</blocks>

		<helpers>
			<training_practice>
				<class>Training_Practice_Helper</class>
			</training_practice>
		</helpers>
	</global>


</config>

