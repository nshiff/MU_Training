<?xml version="1.0" encoding="UTF-8"?>
<config>

	<modules>
		<Training_Practice>
			<version>0.1.0</version>
		</Training_Practice>
	</modules>

	<frontend>
		<routers>
			<training_practice>
				<use>standard</use>
				<args>
					<module>Training_Practice</module>
					<frontName>training</frontName>	<!--	the name is arbitrary
															but by convention it is the module's name
															http://homepage.com/training -->
				</args>
			</training_practice>
		</routers>
		<layout>
			<updates>
				<practice module="Training_Practice">
					<file>practice/layout.xml</file>
				</practice>
			</updates>
		</layout>

	</frontend>



</config>

